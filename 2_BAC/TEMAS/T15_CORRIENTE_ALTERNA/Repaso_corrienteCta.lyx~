\begin_inset Text

\begin_layout Plain Layout
\noindent
\align center

\size small
\begin_inset Formula 
\begin{gather*}
R_{eq}=R_{1}+R_{2}+\cdots+R_{n}\\
V=I\cdot R_{eq}\\
V=V_{1}+V_{2}+\cdots+V_{n}
\end{gather*}

\end_inset


\end_layout

\end_inset
</cell>
