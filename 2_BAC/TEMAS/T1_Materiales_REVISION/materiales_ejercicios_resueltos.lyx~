\begin_layout Section
Ejercicios Resueltos de Ensayo de tracción.
\end_layout

\begin_layout Subsection
Ejercicio 1
\end_layout

\begin_layout Standard
\begin_inset Graphics
	filename ejercicios/ejercicio1.png

\end_inset


\end_layout

\begin_layout Standard
# Tenemos los siguientes datos:
\end_layout

\begin_layout Standard
# Diámetro de la probeta 10mm
\end_layout

\begin_layout Standard
# Longitud de la probeta 100mm
\end_layout

\begin_layout Standard
# instalar el paquete udunits, antes apt-get install libudunits2-dev
\end_layout

\begin_layout Standard
library(units)
\end_layout

\begin_layout Standard
# diámetro
\end_layout

\begin_layout Standard
D = set_units(10,mm)
\end_layout

\begin_layout Standard
# units(D) = with(ud_units,m) #convierte la unidad a metros
\end_layout

\begin_layout Standard
S0=pi*((D/2)^2) # calculo de la superficie S0
\end_layout

\begin_layout Standard
# Longitud
\end_layout

\begin_layout Standard
L0 = set_units(100,mm)
\end_layout

\begin_layout Standard
units(L0) = with(ud_units,m) #convierte la unidad a metros
\end_layout

\begin_layout Standard
# Limite proporcional
\end_layout

\begin_layout Standard
Fp = set_units(3,kN)
\end_layout

\begin_layout Standard
Lp = set_units(0.2,mm)
\end_layout

\begin_layout Standard
Sigp = Fp / S0
\end_layout

\begin_layout Standard
units(Sigp) = with(ud_units, N/m^2)
\end_layout

\begin_layout Standard
Ep = Lp / L0
\end_layout

\begin_layout Standard
# Limite elástico
\end_layout

\begin_layout Standard
Fe = set_units(3.2,kN)
\end_layout

\begin_layout Standard
Le = set_units(0.22,mm)
\end_layout

\begin_layout Standard
Sige = Fe / S0
\end_layout

\begin_layout Standard
units(Sige) = with(ud_units, N/m^2)
\end_layout

\begin_layout Standard
Ee = Le / L0
\end_layout

\begin_layout Standard
# Resistencia a la tracción
\end_layout

\begin_layout Standard
FR = set_units(16,kN)
\end_layout

\begin_layout Standard
LR = set_units(4,mm)
\end_layout

\begin_layout Standard
SigR = FR / S0
\end_layout

\begin_layout Standard
units(SigR) = with(ud_units, N/m^2)
\end_layout

\begin_layout Standard
ER = LR / L0
\end_layout

\begin_layout Standard
# Resistencia a la rotura
\end_layout

\begin_layout Standard
FU = set_units(14,kN)
\end_layout

\begin_layout Standard
LU = set_units(4.5,mm)
\end_layout

\begin_layout Standard
SigU = FU / S0
\end_layout

\begin_layout Standard
units(SigU) = with(ud_units, N/m^2)
\end_layout

\begin_layout Standard
EU = LU / L0
\end_layout

\begin_layout Standard
# representando
\end_layout

\begin_layout Standard
epsilon = c(Ep,Ee,ER,EU)
\end_layout

\begin_layout Standard
sigma = c(Sigp,Sige,SigR,SigU)
\end_layout

\begin_layout Standard
plot(epsilon,sigma
\end_layout

\begin_layout Standard
,pch=22,col="blue"
\end_layout

\begin_layout Standard
,main="Ensayo de tracción - Ejercicio 1"
\end_layout

\begin_layout Standard
,xlim=c(0,0.05), ylim=c(0,2.1e+8))
\end_layout

\begin_layout Standard
segments(0,1,Ep,Sigp,col="red")
\end_layout

\begin_layout Standard
segments(Ee,Sige,Ep,Sigp,col="red")
\end_layout

\begin_layout Standard
segments(Ee,Sige,ER,SigR,col="red")
\end_layout

\begin_layout Standard
segments(EU,SigU,ER,SigR,col="red")
\end_layout

\begin_layout Standard
# Módulo de young 
\end_layout

\begin_layout Standard
E = Sigp / Ep
\end_layout

\begin_layout Standard
cat("El módulo de Young es E = ", as.character(E),as_cf(E))
\end_layout

\begin_layout Standard
units(E) = with(ud_units,GPa)
\end_layout

\begin_layout Standard
cat("El módulo de Young es E = ", as.character(E),as_cf(E))
\end_layout

\begin_layout Standard
# texto
\end_layout

\begin_layout Standard
sigma2=sigma
\end_layout

\begin_layout Standard
units(sigma)= with(ud_units,MPa) #lo pongo en Mega Pascales
\end_layout

\begin_layout Standard
etiquetas = paste0(
\end_layout

\begin_layout Standard
"(",
\end_layout

\begin_layout Standard
round(epsilon,4),
\end_layout

\begin_layout Standard
" , ",
\end_layout

\begin_layout Standard
round(sigma,2),
\end_layout

\begin_layout Standard
as_cf(sigma),
\end_layout

\begin_layout Standard
")")
\end_layout

\begin_layout Standard
posiciones=c(1,3,2,1)
\end_layout

\begin_layout Standard
text(epsilon,sigma2,etiquetas,col="blue",pos=posiciones,cex=0.8)
\end_layout

\begin_layout Standard
text(0.03,sigma2[1],paste0("El módulo de Young es E = ", as.character(round(E,2)),
as_cf(E)),col="black")
\end_layout

\begin_layout Subsection
Ejercicio 2
\end_layout

\begin_layout Standard
Añadir al script anterior de R STUDIO lo siguiente
\end_layout

\begin_layout Standard
# Trabajo bajo la curva
\end_layout

\begin_layout Standard
W1=Fp*Lp/2
\end_layout

\begin_layout Standard
W2=Fp*(Le-Lp)
\end_layout

\begin_layout Standard
W3=(Fe-Fp)*(Le-Lp)/2
\end_layout

\begin_layout Standard
W4=Fe*(LR-Le)
\end_layout

\begin_layout Standard
W5=(FR-Fe)*(LR-Le)/2
\end_layout

\begin_layout Standard
W6=FU*(LU-LR)
\end_layout

\begin_layout Standard
W7=(FR-FU)*(LU-LR)/2
\end_layout

\begin_layout Standard
W=W1+W2+W3+W4+W5+W6+W7
\end_layout

\begin_layout Standard
units(W)=with(ud_units,J)
\end_layout

\begin_layout Standard
cat("El trabajo realizado es W=",as.character(W),as_cf(W))
\end_layout

\begin_layout Standard
text(0.03,sigma2[1]/2,paste0("El trabajo realizado es W=",as.character(W),as_cf(W)
),col="black")
\end_layout

\begin_layout Subsection
Ejercicio 3
\end_layout

\begin_layout Standard
\begin_inset Flex Chunk
