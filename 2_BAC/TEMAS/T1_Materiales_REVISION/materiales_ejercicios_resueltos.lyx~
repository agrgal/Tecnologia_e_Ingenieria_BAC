\begin_layout Plain Layout

# Barra cuadrada
\end_layout

\begin_layout Plain Layout

# instalar el paquete udunits, antes apt-get install libudunits2-dev
\end_layout

\begin_layout Plain Layout

library(units)
\end_layout

\begin_layout Plain Layout

# Lado y L0
\end_layout

\begin_layout Plain Layout

a = set_units(1,cm)
\end_layout

\begin_layout Plain Layout

L0 = set_units(10,cm)
\end_layout

\begin_layout Plain Layout

# Superficie
\end_layout

\begin_layout Plain Layout

S0=a^2 # Ya que es cuadrado
\end_layout

\begin_layout Plain Layout

# Fuerza de Tracción
\end_layout

\begin_layout Plain Layout

F = set_units(8,kN)
\end_layout

\begin_layout Plain Layout

# Módulo de Young
\end_layout

\begin_layout Plain Layout

E=set_units(2,MN/cm^2)
\end_layout

\begin_layout Plain Layout

# Límite de proporcionalidad
\end_layout

\begin_layout Plain Layout

SigP =set_units(100,MPa)
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

# --- Resolución ----
\end_layout

\begin_layout Plain Layout

Sigma = F / S0
\end_layout

\begin_layout Plain Layout

epsilon = Sigma / E
\end_layout

\begin_layout Plain Layout

units(Sigma)=with(ud_units,MPa)
\end_layout

\begin_layout Plain Layout

deltaL=epsilon*L0
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

# Carga de 80 kN
\end_layout

\begin_layout Plain Layout

F2 = set_units(80,kN)
\end_layout

\begin_layout Plain Layout

Sigma2 = F2 /S0
\end_layout

\begin_layout Plain Layout

Sigma2
\end_layout

\begin_layout Plain Layout

units(Sigma2)=with(ud_units,MPa) # cambiar a Mega Pascales
\end_layout

\begin_layout Plain Layout

Sigma2
\end_layout

\end_inset


\end_layout

