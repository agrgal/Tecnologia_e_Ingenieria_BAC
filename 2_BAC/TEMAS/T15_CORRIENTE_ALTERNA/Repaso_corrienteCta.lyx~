\align center

\size small
\begin_inset Formula 
\begin{gather*}
V_{1}=I\cdot R_{1}\\
V_{2}=I\cdot R_{2}\\
\vdots\\
V_{n}=I\cdot R_{n}
\end{gather*}

\end_inset


\end_layout

\end_inset
