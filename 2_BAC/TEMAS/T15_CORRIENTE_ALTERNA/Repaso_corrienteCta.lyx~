\end_inset


\end_layout

\begin_layout Plain Layout
Siendo 
\begin_inset Quotes cld
\end_inset

h
\begin_inset Quotes crd
\end_inset

 su longitud, 
\begin_inset Quotes cld
\end_inset

S
\begin_inset Quotes crd
\end_inset

 su sección y 
\begin_inset Quotes cld
\end_inset

N
\begin_inset Quotes crd
